<div class="navbar navbar-inverse navbar-fixed-top main-menu">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{url "/"}}"><img src="{{url "/themes/theme1/ui/img/identity-logo.png"}}"/>
        </a>
    </div>
    <div class="navbar-collapse collapse main-menu-inside">

        <ul class="nav navbar-nav pull-left menu1">
            {{#if login}}
            {{else}}

			{{{logReqLinks}}}
    
            {{/if}}


        </ul>
        <ul class="nav navbar-nav pull-right menu2">
        
        	{{#if username}}
        	<li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="icon-user"></i><br> {{username}} <b
                        class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li>
                        <a href="my_profiles.jag">{{messages.view_profile}}</a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="controllers/login-logout/logout.jag">
                            <i class="icon-signout"></i>
                            <span>{{messages.logout}}</span></a>
                    </li>
                </ul>
            </li>
        	
        	
        	{{else}}
			<li>
			<a href="login.jag" class="" ><i class="icon-user"></i><span><br/>{{messages.login}}</span></a>
        		
        	</li>		
        	{{/if}}
            

            {{{logNotReqLinks}}}
        </ul>
    </div>
    <!--/.navbar-collapse -->
</div>
<div class="breadcrumb-strip">
<div class="container">
    <!-- Example row of columns -->
    <div class="row">
        <div class="col-lg-12">
            <ul class="breadcrumb">
                {{#each breadcrumb }}
                    {{#if this.isLink}}
                        <li><a href="{{url this.link}}">{{this.name}}</a> <span class="divider"></span></li>
                    {{else}}
                        <li class="active">{{this.name}}</li>
                    {{/if}}
                {{/each}}
            </ul>
        </div>
    </div>
</div>
</div>
        </div>